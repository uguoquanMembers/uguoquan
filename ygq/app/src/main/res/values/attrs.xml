<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IRecyclerView">
        <attr name="refreshHeaderLayout" format="reference" />
        <attr name="loadMoreFooterLayout" format="reference" />
        <attr name="refreshEnabled" format="boolean" />
        <attr name="loadMoreEnabled" format="boolean" />
        <attr name="refreshFinalMoveOffset" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundCornerImageView">
        <!--圆角位置-->
        <attr name="roundCornerLeftTop" format="boolean"/>
        <attr name="roundCornerLeftBottom" format="boolean"/>
        <attr name="roundCornerRightTop" format="boolean"/>
        <attr name="roundCornerRightBottom" format="boolean"/>
        <!--圆角大小-->
        <attr name="roundCornerRadius" format="dimension"/>
    </declare-styleable>
</resources>